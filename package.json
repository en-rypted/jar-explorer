{
  "name": "jar-explorer",
  "displayName": "Jar Explorer",
  "description": "This used to view the class file content in vs code",
  "repository": {
    "type": "git",
    "url": "https://github.com/en-rypted/jar-explorer.git"
  },
  "publisher": "shivwakchaure",
  "author": {
    "name": "Shiv",
    "email": "shivwakchaure19@gmail.com"
  },
  "version": "1.1.0",
  "engines": {
    "vscode": "^1.102.0"
  },
  "icon": "media/icon.png",
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.102.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.25.1"
  },
  "contributes": {
    "views": {
      "jarExplorer": [
        {
          "id": "jarExplorerView",
          "name": "JAR Explorer",
          "icon": "media/icon.png"
        }
      ]
    },
    "customEditors": [
      {
        "viewType": "jarExplorer.editor",
        "displayName": "JAR Explorer",
        "selector": [
          {
            "filenamePattern": "*.jar"
          },
          {
            "filenamePattern": "*.zip"
          },
          {
            "filenamePattern": "*.war"
          },
          {
            "filenamePattern": "*.ear"
          },
          {
            "filenamePattern": "*.vsix"
          }
        ]
      }
    ],
    "configuration": {
      "properties": {
        "jarExplorer.jdkPath": {
          "type": "string",
          "default": "java",
          "description": "Path to your Java executable"
        }
      }
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "jarExplorer",
          "title": "JAR Explorer",
          "icon": "media/icon_nav.png"
        }
      ]
    },
    "commands": [
      {
        "command": "jarExplorer.openWithCustomEditor",
        "title": "üß© Open in JAR Explorer"
      },
      {
        "command": "jarExplorer.removeFile",
        "title": "üóëÔ∏è Remove Folder"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "jarExplorer.openWithCustomEditor",
          "when": "resourceExtname == .jar || resourceExtname == .zip || resourceExtname == .war || resourceExtname == .ear || resourceExtname == .vsix",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "jarExplorer.removeFile",
          "when": "view == jarExplorerView && viewItem == jarRoot",
          "group": "navigation"
        }
      ]
    }
  }
}